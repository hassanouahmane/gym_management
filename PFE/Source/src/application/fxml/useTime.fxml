<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="497.0" prefWidth="750.0" stylesheets="@../css/general.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UseTimeController">
   <children>
      <TableView fx:id="scheduleTab" layoutX="10.0" layoutY="10.0" maxWidth="-Infinity" styleClass="normal-box" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="75.0">
         <columns>
            <TableColumn fx:id="cscheduleId" maxWidth="1000.0" minWidth="-1.0" prefWidth="150.0" text="Id" />
            <TableColumn fx:id="ccoach" maxWidth="1000.0" minWidth="-1.0" prefWidth="420.0" text="Coach" />
            <TableColumn fx:id="cstart" maxWidth="1000.0" minWidth="-1.0" prefWidth="300.0" text="Start" />
            <TableColumn fx:id="cend" maxWidth="1000.0" minWidth="-1.0" prefWidth="300.0" text="End" />
            <TableColumn fx:id="cday" maxWidth="1000.0" minWidth="-1.0" prefWidth="300.0" text="Day" />
            <TableColumn fx:id="csalle" maxWidth="1000.0" minWidth="-1.0" prefWidth="300.0" text="Salle" />
            <TableColumn fx:id="cnote" maxWidth="1000.0" minWidth="-1.0" prefWidth="300.0" text="Note" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
         <children>
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="addBtn" mnemonicParsing="false" onAction="#addSchedule" prefWidth="150.0" text="Add Schedule" />
         </children>
         <padding>
            <Insets right="10.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</AnchorPane>
