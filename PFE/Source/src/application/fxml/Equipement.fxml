<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EquipementController">
   <content>
      <AnchorPane prefHeight="591.0" prefWidth="1109.0">
         <children>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" layoutX="-28.0" layoutY="-6.0" orientation="VERTICAL" prefHeight="600.0" prefWidth="1098.0" style="-fx-background-color: white;">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
               <children>
                  <HBox spacing="20.0">
                     <children>
                        <Button fx:id="addequipementbtn" mnemonicParsing="false" onAction="#equipement" style="-fx-background-color: #70a1ff;" text="Add Equipment " />
                        <Button fx:id="selebtn" mnemonicParsing="false" onAction="#sele" style="-fx-background-color: #70a1ff;" text="Sele" />
                     </children>
                  </HBox>
                  <VBox>
                     <children>
                        <TableView fx:id="tableView" prefHeight="495.0" prefWidth="969.0" stylesheets="@../css/tableau.css">
                           <columns>
                              <TableColumn fx:id="equipementColumn" prefWidth="235.0" text="equipement " />
                              <TableColumn fx:id="quantityColumn" prefWidth="300.0" text="quantity" />
                              <TableColumn fx:id="priceColumn" prefWidth="175.0" text="price" />
                              <TableColumn fx:id="discriptionColumn" prefWidth="258.0" text="discription" />
                           </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </FlowPane>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
